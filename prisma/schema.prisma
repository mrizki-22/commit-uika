// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Anggota {
  id Int @id @default(autoincrement())
  nama String
  fakultas String
  prodi String
  no_telp String
  email String
  alamat String
  angkatan String
  status String
}